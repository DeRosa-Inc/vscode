	"displayName": "%displayName%",
	"description": "%description%",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"enableProposedApi": true,
	"activationEvents": [
		"*",
		"onAuthenticationRequest:microsoft"
	],
	"extensionKind": [
		"ui",
		"workspace"
	],
	"aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
